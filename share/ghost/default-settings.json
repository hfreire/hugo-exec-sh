  "blog": {
    "title": {
      "defaultValue": "Hugo Freire",
      "validations": {
        "isLength": {
          "max": 150
        }
      }
    },
    "description": {
      "defaultValue": "",
      "validations": {
        "isLength": {
          "max": 200
        }
      }
    },
    "logo": {
      "defaultValue": "/content/images/2019/01/logo.png"
    },
    "cover_image": {
      "defaultValue": ""
    },
    "icon": {
      "defaultValue": "/content/images/2019/01/icon.png"
    },
    "default_locale": {
      "defaultValue": "en",
      "validations": {
        "isEmpty": false
      }
    },
    "active_timezone": {
      "defaultValue": "Europe/Amsterdam",
      "validations": {
        "isTimezone": true,
        "isEmpty": false
      }
    },
    "force_i18n": {
      "defaultValue": "true",
      "validations": {
        "isEmpty": false,
        "isIn": [
          [
            "true",
            "false"
          ]
        ]
      }
    },
    "permalinks": {
      "defaultValue": "/:slug/",
      "validations": {
        "matches": "^(\/:?[a-z0-9_-]+){1,5}\/$",
        "matches": "(:id|:slug|:year|:month|:day|:author|:primary_tag|:primary_author)",
        "notContains": "/ghost/"
      }
    },
    "amp": {
      "defaultValue": "true",
      "validations": {
        "isIn": [
          [
            "true",
            "false"
          ]
        ]
      }
    },
    "ghost_head": {
